{
    "rewrites": [
        {
            "source": "/cnb/:path*",
            "destination": "https://www.cnb.cz/:path*"
        }
    ],
    "headers": [
        {
            "source": "/cnb/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Accept"
                }
            ]
        }
    ]
}