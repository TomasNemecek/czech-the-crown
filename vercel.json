{
  "rewrites": [
    {
      "source": "/cnb/(.*)",
      "destination": "https://www.cnb.cz/$1"
    }
  ],
  "headers": [
    {
      "source": "/cnb/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=900, stale-while-revalidate"
        }
      ]
    }
  ]
}
